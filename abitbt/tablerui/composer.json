{
    "name": "abitbt/tablerui",
    "description": "UI component library for tabler.",
    "keywords": ["tabler", "laravel", "blade", "components", "ui"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abi",
            "email": "calebporzio@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "secondnetwork/blade-tabler-icons": "^3.35"
    },
    "autoload": {
        "psr-4": {
            "Tabler\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tabler\\TablerServiceProvider"
            ],
            "aliases": {
                "Tabler": "Tabler\\Tabler"
            }
        }
    }
}
